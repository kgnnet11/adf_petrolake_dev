{
	"name": "pl_WorldBankIncrementalLoad_Energy",
	"properties": {
		"activities": [
			{
				"name": "LookupLastLoadedYear",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "SELECT LastLoadedYear\nFROM dbo.WorldBankIncrementalLoadControl\nWHERE CountryCode = @CountryCode\n  AND IndicatorCode = @IndicatorCode;",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"dataset": {
						"referenceName": "ds_worldbank_controldata_energy_new",
						"type": "DatasetReference",
						"parameters": {
							"CountryCode": {
								"value": "@pipeline().parameters.CountryCode",
								"type": "Expression"
							},
							"IndicatorCode": {
								"value": "@pipeline().parameters.IndicatorCode",
								"type": "Expression"
							}
						}
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "GetTotalPages",
				"type": "WebActivity",
				"dependsOn": [
					{
						"activity": "LookupLastLoadedYear",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "GET",
					"url": "https://api.worldbank.org/v2/country/IN/indicator/EG.USE.PCAP.KG.OE?format=json&page=1"
				}
			}
		],
		"parameters": {
			"LastLoadedYear": {
				"type": "int",
				"defaultValue": 2023
			},
			"CurrentPage": {
				"type": "int",
				"defaultValue": 1
			},
			"LoadDate": {
				"type": "string",
				"defaultValue": "'2026-02-21'"
			},
			"CountryCode": {
				"type": "string",
				"defaultValue": "IN"
			},
			"IndicatorCode": {
				"type": "string",
				"defaultValue": "EG.USE.PCAP.KG.OE"
			}
		},
		"annotations": []
	}
}